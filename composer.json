        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2-imagine": "^2.2",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "zelenin/yii2-slug-behavior": "~1.5.1",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "vova07/yii2-imperavi-widget": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "yurkinx/yii2-image": "dev-master",
        "kartik-v/yii2-widget-fileinput": "1.0.8",
        "paulzi/yii2-nested-sets" : "^1.0",
        "yiisoft/yii2-authclient": "^2.1",
        "kotchuprik/yii2-odnoklassniki-authclient": "*",
        "electrolinux/phpquery": "^0.9.6",
        "skeeks/yii2-mobile-detect": "^1.0",
        "yiisoft/yii2-redis": "^2.0",
        "kartik-v/yii2-widget-colorinput": "*",
        "aki/yii2-bot-telegram": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
        ,
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
